<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="400" d:DesignHeight="250"
        Width="400" Height="250"
        x:Class="Call_of_Duty_HQ.Views.Popup"
        Title="Popup" >
    <StackPanel>
        <TextBlock x:Name="PopupTitle" FontSize="16" FontWeight="Bold" Text="Popup Title" Margin="5 5 0 0"/>
        <TextBlock x:Name="PopupMessage" Text="Popup Message" Margin="5 15 0 0"/>
        <StackPanel Orientation="Horizontal" VerticalAlignment="Bottom" HorizontalAlignment="Right" Height="350">
            <Button x:Name="CancelButton" Content="Cancel"/>
            <Button x:Name="PrimaryButton" Content="Confirm"/>
        </StackPanel>
    </StackPanel>
</Window>
