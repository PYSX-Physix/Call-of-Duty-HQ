                <Button x:Name="___CallofDutyLaunch___BO_3" Content="Call of Duty: Black Ops III" HorizontalAlignment="Left" Height="23" Margin="0,153,0,0" VerticalAlignment="Top" Width="175" Click="___CallofDutyLaunch___BO_3_Clicked" Grid.Column="2"/>
                <Button x:Name="___CallofDutyLaunch___IW" Content="Call of Duty: Infinite Warfare" HorizontalAlignment="Left" Height="23" Margin="14,176,0,0" VerticalAlignment="Top" Width="175" Click="___CallofDutyLaunch___IW_Clicked" Grid.ColumnSpan="2"/>
                <Button x:Name="___CallofDutyLaunch___WW2" Content="Call of Duty: WWII" HorizontalAlignment="Left" Height="23" VerticalAlignment="Top" Width="175" Margin="0,176,0,0" Click="___CallofDutyLaunch___WW2_Clicked" Grid.Column="2"/>
                <Button x:Name="___CallofDutyLaunch___BOCW" Content="Call of Duty: Black Ops Cold War" HorizontalAlignment="Left" Height="23" Margin="0,199,0,0" VerticalAlignment="Top" Width="175" Click="___CallofDutyLaunch___BOCW_Clicked" Grid.Column="2"/>
                <Button x:Name="___CallofDutyLaunch__VG" Content="Call of Duty: Vangaurd" HorizontalAlignment="Left" Height="23" Margin="14,222,0,0" VerticalAlignment="Top" Width="175" Click="___CallofDutyLaunch__VG_Click" Grid.ColumnSpan="2"/>
