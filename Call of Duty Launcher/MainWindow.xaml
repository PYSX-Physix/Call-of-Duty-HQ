                <Button x:Name="___CallofDutyLaunch___G" Content="Call of Duty: Ghosts" Margin="0,130,0,0" Width="175" Click="___CallofDutyLaunch___G_Clicked" RenderTransformOrigin="0.5,0.5" HorizontalAlignment="Left" VerticalAlignment="Top" Height="23" Grid.Column="2">
                    <Button.RenderTransform>
                        <TransformGroup>
                            <ScaleTransform ScaleY="1"/>
                            <SkewTransform/>
                            <RotateTransform/>
                            <TranslateTransform/>
                        </TransformGroup>
                    </Button.RenderTransform>
                </Button>
                <Button x:Name="___CallofDutyLaunch__MW_19" Content="Call of Duty: Modern Warfare" HorizontalAlignment="Left" Height="23" Margin="14,199,0,0" VerticalAlignment="Top" Width="175" Click="___CallofDutyLaunch__MW_19_Clicked" Grid.ColumnSpan="2"/>
                <Button x:Name="___CallofDutyLaunch__MW2_22" Content="Call of Duty: Modern Warfare II" HorizontalAlignment="Left" Height="23" Margin="0,222,0,0" VerticalAlignment="Top" Width="175" Click="___CallofDutyLaunch__MW2_22_Clicked" Grid.Column="2"/>
