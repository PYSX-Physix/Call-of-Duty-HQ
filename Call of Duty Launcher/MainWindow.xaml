        <TextBlock HorizontalAlignment="Left" Height="25" Margin="648,457,0,0" TextWrapping="Wrap" Text="Derail" VerticalAlignment="Top" Width="163" Foreground="White" FontSize="18" FontWeight="Bold"/>
        <TextBlock HorizontalAlignment="Left" Height="25" Margin="10,598,0,0" TextWrapping="Wrap" Text="High Rise" VerticalAlignment="Top" Width="163" Foreground="White" FontSize="18" FontWeight="Bold"/>
