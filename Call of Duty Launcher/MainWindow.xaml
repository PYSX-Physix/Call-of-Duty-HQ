<Window x:Class="Call_of_Duty_Launcher.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Call_of_Duty_Launcher"
        mc:Ignorable="d"
            Title="Call of Duty HQ" Background="#FF1F1F1F" Icon="/Images/CallofDutyLogo.ico" MaxHeight="1000" MaxWidth="1500" WindowStartupLocation="CenterScreen" ResizeMode="CanMinimize" Height="800" Width="1000">
    <Grid Margin="10,10,10,1">
        <Image x:Name="Title_Image" Source="/Images/CoD Logo.png" Stretch="Fill" Margin="315,10,352,672"/>
        <Button Content="About" Height="45" Width="41" HorizontalAlignment="Left" VerticalAlignment="Top" Click="Button_Click" Margin="14,10,0,0"/>
        <Button Content="Help" Height="45" Width="41" Click="Button_Click_Help" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="76,10,0,0"/>
        <TextBlock Margin="294,101,259,622" TextWrapping="Wrap" Text="This application depends on the Steam Launcher to work properly." Foreground="White"/>
        <TextBlock TextWrapping="Wrap" Text="News" Width="65" Foreground="White" FontSize="24" FontWeight="Bold" Height="30" Margin="228,101,686,642"/>
        <Image Margin="94,145,490,425" Source="/Images/Game_Backgrounds/CoDMWIII.jpg" Stretch="Fill"/>
