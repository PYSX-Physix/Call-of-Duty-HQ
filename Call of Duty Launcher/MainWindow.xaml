                    <ColumnDefinition Width="0*"/>
                    <ColumnDefinition Width="63*"/>
                    <ColumnDefinition Width="62*"/>
                </Grid.ColumnDefinitions>
                <TextBlock HorizontalAlignment="Left" Height="30" TextWrapping="Wrap" Text="Games" VerticalAlignment="Top" Width="88" TextAlignment="Center" FontSize="24" Foreground="White" FontWeight="Bold" Margin="142,-42,0,0" Grid.ColumnSpan="3"/>
                <Button x:Name="___CallofDutyLaunch_" Content="Call of Duty" HorizontalAlignment="Left" Height="23" Margin="14,39,0,0" VerticalAlignment="Top" Width="175" Click="___CallofDutyLaunch_Clicked" Grid.ColumnSpan="2"/>
                <Button x:Name="___CallofDutyLaunchUO__" Content="Call of Duty: United Offensive" HorizontalAlignment="Left" Height="23" Margin="14,62,0,0" VerticalAlignment="Top" Width="175" Click="___CallofDutyLaunchUO__Clicked" Grid.ColumnSpan="2"/>
                <Button x:Name="___CallofDutyLaunch2__" Content="Call of Duty 2" HorizontalAlignment="Left" Height="23" Margin="0,39,0,0" VerticalAlignment="Top" Width="175" Click="___CallofDutyLaunch2__Clicked" Grid.Column="2"/>
