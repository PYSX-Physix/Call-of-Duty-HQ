                <Button x:Name="___CallofDutyLaunch___MW1" Content="Call of Duty 4: Modern Warfare" HorizontalAlignment="Left" Height="23" Margin="0,62,0,0" VerticalAlignment="Top" Width="175" Click="___CallofDutyLaunch___MW_Clicked" Grid.Column="2"/>
