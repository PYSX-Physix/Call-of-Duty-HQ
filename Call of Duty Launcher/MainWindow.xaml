        <TextBlock HorizontalAlignment="Left" Height="25" Margin="648,598,0,0" TextWrapping="Wrap" Text="Favela" VerticalAlignment="Top" Width="163" Foreground="White" FontSize="18" FontWeight="Bold"/>
