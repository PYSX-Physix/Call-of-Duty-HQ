        <TextBlock HorizontalAlignment="Left" Height="25" Margin="350,409,0,0" TextWrapping="Wrap" Text="New Maps" VerticalAlignment="Top" Width="243" Foreground="White" FontSize="18" FontWeight="Bold" TextAlignment="Center"/>
